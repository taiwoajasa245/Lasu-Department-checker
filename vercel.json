{
  "version": 2,
  "builds": [
    {
      "src": "server.js", 
      "use": "@vercel/node" 
    },
    {
      "src": "public/**", 
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "spec/$1"
    },
    {
      "src": "/((?!api-docs).*)",
      "dest": "server.js" 
    }
  ]
}
